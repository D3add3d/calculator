{
  "name": "calculatorjs",
  "version": "1.0.0",
  "description": "Angular2 calculator component",
  "main": "app.js",
  "scripts": {
    "start": "npm run webpack-dev-server",
    "build": "npm install && npm run postinstall && npm run rimraf && npm run webpack:production && node server.js",
    "test": "karma start",
    "typings": "typings",
    "postinstall": "npm run typings install",
    "heroku-prebuild": "echo task that is run before npm install is called",
    "heroku-postbuild": "npm run webpack-production",
    "webpack-production": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "rimraf": "rimraf dist",
    "webpack-dev-server": "webpack-dev-server --inline --progress --port 8080 --hot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyriter/calculator.git"
  },
  "author": "Phong Vuong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pyriter/calculator/issues"
  },
  "homepage": "https://github.com/pyriter/calculator",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "express": "^4.14.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "typings": "^1.0.4",
    "typescript": "^1.8.10",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.9.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "rimraf": "^2.5.2",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "webpack-dev-server": "^1.14.1"
  }
}
